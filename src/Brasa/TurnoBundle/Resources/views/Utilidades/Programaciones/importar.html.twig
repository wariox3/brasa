{% extends '::base.html.twig' %}
{% block title %}Importar programacion - Soga App{% endblock %}
{% block content %}
<script>
    $(function () {
      $('[data-toggle="tooltip"]').tooltip()
    })
</script>    
<div class="container">
    <div class="page-header">
        <h3>Importar programaciones</h3>
    </div>
    {{ form_start(form, { 'attr': {'class': 'form-horizontal', 'novalidate': 'novalidate'} }) }}
        {{ form_errors(form) }}
    
        <div class="panel panel-default">
            <div class="panel-heading">Cargar archivo</div>
            <div class="panel-body">
                <div>                      
                    <div class="form-group form-group-sm">
                        <label for="form_numeroIdentificacÃ³n" class="col-sm-2 control-label">Cargar:</label>
                        <div class="col-sm-10">
                            {{ form_widget(form.attachment, { 'attr': {'class': 'btn btn-sm btn-default'} }) }}            
                        </div>
                    </div>                                
                </div>                                            
            </div>
        </div>                   
        <div class="modal-footer">        
                {{ form_widget(form.BtnCargar, { 'attr': {'class': 'btn btn-sm btn-primary'} }) }}            
        </div> 
        <table width="100%" class="table table-condensed table-bordered">
            <tr>           
                <th>RECURSO</th>
                <th>1</th>
                <th>2</th>
                <th>3</th>
                <th>4</th>
