                    <td>{{ arProgramacionesImportar.dia27  }}</td>
                    <td>{{ arProgramacionesImportar.dia28  }}</td>
                    <td>{{ arProgramacionesImportar.dia29  }}</td>
                    <td>{{ arProgramacionesImportar.dia30  }}</td>
                    <td>{{ arProgramacionesImportar.dia31  }}</td>                                         
                </tr>
            {% endfor %}
        </table>  
        <div class="btn-group" style="float: left">
                <div class="btn btn-default btn-sm">Registros <span class="badge">{{ arProgramacionesImportar.getTotalItemCount }}</span></div>                        
        </div>
        <div class="btn-group btn-sm" style="float: left;margin-left: 0px;padding-top: 0px;">
                {{ knp_pagination_render(arProgramacionesImportar) }}       
        </div>
        <div class="btn-toolbar" style="float: right">
            <div class="btn-group">                
            </div>
        </div>        
