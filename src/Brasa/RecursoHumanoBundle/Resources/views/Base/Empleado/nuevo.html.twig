{% extends '::base2.html.twig' %}
{% block title %}Empleado nuevo - Soga App{% endblock %}
{% block content %}
<div class="container">
    <script type="text/javascript">
    $(document).ready(function() {
            $('.date').datepicker({ dateFormat: 'yy-mm-dd' ,dayNamesMin: [ "Do", "Lu", "Ma", "Mi", "Ju", "Vi", "Sa" ],monthNames: [ "Enero", "Febrero", "Marzo", "Abril", "Mayo", "Junio", "Julio", "Agosto", "Septiembre", "Octubre", "Noviembre", "Deciembre" ], monthNames: [ "Enero", "Febrero", "Marzo", "Abril", "Mayo", "Junio", "Julio", "Agosto", "Septiembre", "Octubre", "Noviembre", "Diciembre" ]})  });
    </script>
    {{ form_start(form, { 'attr': {'class': 'form-horizontal'} }) }}
    {{ form_errors(form) }}

    <h3>Nuevo empleado</h3>
    <div class="panel panel-default">
        <div class="panel-heading">Informacion general</div>
        <div class="panel-body">
            <div class="form-group form-group-sm">
                <label for="form_tipoIdentificacionRel" class="col-sm-2 control-label">Tipo:</label>
                <div class="col-sm-10">
                        {{ form_widget(form.tipoIdentificacionRel, { 'attr': {'class': 'form-control'} }) }}
                </div>
            </div>
            <div class="form-group form-group-sm">
                <label for="form_numeroIdentificacion" class="col-sm-2 control-label">Identificacion:</label>
                <div class="col-sm-10">
                        {% if form_errors(form.numeroIdentificacion) %}
                            {{ form_widget(form.numeroIdentificacion, { 'attr': {'class': 'form-control'} }) }}
                            <div class="alert alert-danger" role="alert">{{ form_errors(form.numeroIdentificacion)}}</div>
                        {% else %}
                            {{ form_widget(form.numeroIdentificacion, { 'attr': {'class': 'form-control'} }) }}
                        {% endif %}
                </div>
            </div>   
            <div class="form-group form-group-sm">
                <label for="form_fechaExpedicionIdentificacion" class="col-sm-2 control-label">Fecha expedición:</label>
                <div class="col-sm-10">
                    {{ form_widget(form.fechaExpedicionIdentificacion, {'attr': {'maxlength' : 10} }) }} 
                </div>                
            </div>    
            <div class="form-group form-group-sm">
                <label for="form_ciudadExpedicionRel" class="col-sm-2 control-label">Ciudad expedición:</label>
                <div class="col-sm-10">
                        {{ form_widget(form.ciudadExpedicionRel, { 'attr': {'class': 'form-control'} }) }}
                </div>
            </div>    
            <div class="form-group form-group-sm">
                <label for="form_nombre1" class="col-sm-2 control-label">Nombre 1:</label>
                <div class="col-sm-10">
                        
                    {{ form_widget(form.nombre1, { 'attr': {'style':'text-transform:uppercase;','onblur':'javascript:this.value=this.value.toUpperCase()','class': 'form-control'} }) }}
                    
                </div>
            </div>
            <div class="form-group form-group-sm">
                <label for="form_nombre2" class="col-sm-2 control-label">Nombre 2:</label>
                <div class="col-sm-10">
                        {{ form_widget(form.nombre2, { 'attr': {'style':'text-transform:uppercase;','onblur':'javascript:this.value=this.value.toUpperCase()','class': 'form-control'} }) }}
                </div>
            </div>
            <div class="form-group form-group-sm">
                <label for="form_apellido1" class="col-sm-2 control-label">Apellido 1:</label>
                <div class="col-sm-10">
                        {{ form_widget(form.apellido1, { 'attr': {'style':'text-transform:uppercase;','onblur':'javascript:this.value=this.value.toUpperCase()','class': 'form-control'} }) }}
                </div>
            </div>
            <div class="form-group form-group-sm">
                <label for="form_apellido2" class="col-sm-2 control-label">Apellido 2:</label>
                <div class="col-sm-10">
                        {{ form_widget(form.apellido2, { 'attr': {'style':'text-transform:uppercase;','onblur':'javascript:this.value=this.value.toUpperCase()','class': 'form-control'} }) }}
                </div>
            </div>
            <div class="form-group form-group-sm">
                <label for="form_codigoSexoFk" class="col-sm-2 control-label">Sexo:</label>
                <div class="col-sm-10">
                        {{ form_widget(form.codigoSexoFk, { 'attr': {'class': 'form-control'} }) }}
                </div>
            </div>
            <div class="form-group form-group-sm">
                <label for="form_estadoCivilRel" class="col-sm-2 control-label">Estado civil:</label>
                <div class="col-sm-10">
                        {{ form_widget(form.estadoCivilRel, { 'attr': {'class': 'form-control'} }) }}
                </div>
            </div>
            <div class="form-group form-group-sm">
                <label for="form_rhRel" class="col-sm-2 control-label">Rh:</label>
                <div class="col-sm-10">
                        {{ form_widget(form.rhRel, { 'attr': {'class': 'form-control'} }) }}
                </div>
            </div>
            <div class="form-group form-group-sm">
                <label for="form_rhRel" class="col-sm-2 control-label">Libreta militar:</label>
                <div class="col-sm-10">
                        {{ form_widget(form.codigoTipoLibreta, { 'attr': {'class': 'form-control'} }) }}
                </div>
            </div>    
            <div class="form-group form-group-sm">
                <label for="form_fechaNacimiento" class="col-sm-2 control-label">Fecha nacimiento:</label>
                <div class="col-sm-10">
                    {{ form_widget(form.fechaNacimiento, {'attr': {'maxlength' : 10} }) }} 
                </div>                
            </div>    
            <div class="form-group form-group-sm">
                <label for="form_ciudadNacimientoRel" class="col-sm-2 control-label">Ciudad nacimiento:</label>
                <div class="col-sm-10">
                        {{ form_widget(form.ciudadNacimientoRel, { 'attr': {'class': 'form-control'} }) }}
                </div>
            </div>        
            <div class="form-group form-group-sm">
                <label for="form_padreFamilia" class="col-sm-2 control-label">Padre de familia</label>
                <div class="col-sm-10">
                        {{ form_widget(form.padreFamilia, { 'attr': {'class': 'form-control'} }) }}
                        <div class="checkbox">
                            <label>
                                <input type="checkbox" name="ChkCabezaHogar" {% if arEmpleado.CabezaHogar == 1 %} checked="true" {% endif %}> Cabeza de hogar
                            </label>
                        </div>
                </div>
            </div>
            <div class="form-group form-group-sm">
                <label for="form_ciudadNacimientoRel" class="col-sm-2 control-label">Nivel de estudio:</label>
                <div class="col-sm-10">
                        {{ form_widget(form.empleadoEstudioTipoRel, { 'attr': {'class': 'form-control'} }) }}
                </div>
            </div>            
            <div class="form-group form-group-sm">
                <label for="form_telefono" class="col-sm-2 control-label">Telefono:</label>
                <div class="col-sm-10">
                        {{ form_widget(form.telefono, { 'attr': {'class': 'form-control'} }) }}
                </div>
            </div>
            <div class="form-group form-group-sm">
                <label for="form_celular" class="col-sm-2 control-label">Celular:</label>
                <div class="col-sm-10">
                        {{ form_widget(form.celular, { 'attr': {'class': 'form-control'} }) }}
                </div>
            </div>
            <div class="form-group form-group-sm">
                <label for="form_correo" class="col-sm-2 control-label">Correo:</label>
                <div class="col-sm-10">
                        {{ form_widget(form.correo, { 'attr': {'class': 'form-control'} }) }}
                </div>
            </div>                
            <div class="form-group form-group-sm">
                <label for="form_direccion" class="col-sm-2 control-label">Direccion:</label>
                <div class="col-sm-10">
                        {{ form_widget(form.direccion, { 'attr': {'style':'text-transform:uppercase;','onblur':'javascript:this.value=this.value.toUpperCase()','class': 'form-control'} }) }}
                </div>
            </div> 
            <div class="form-group form-group-sm">
                <label for="form_ciudadRel" class="col-sm-2 control-label">Ciudad residencia:</label>
                <div class="col-sm-10">
                        {{ form_widget(form.ciudadRel, { 'attr': {'class': 'form-control'} }) }}
                </div>
            </div>
            <div class="form-group form-group-sm">
                <label for="form_barrio" class="col-sm-2 control-label">Barrio:</label>
                <div class="col-sm-10">
                        {{ form_widget(form.barrio, { 'attr': {'style':'text-transform:uppercase;','onblur':'javascript:this.value=this.value.toUpperCase()','class': 'form-control'} }) }}
                </div>
            </div>
            <div class="form-group form-group-sm">
                <label for="form_discapacidad" class="col-sm-2 control-label">Discapacidad:</label>
                <div class="col-sm-10">
                        {{ form_widget(form.discapacidad, { 'attr': {'class': 'form-control'} }) }}
                </div>
            </div>    
            <div class="form-group form-group-sm">
                <label for="form_camisa" class="col-sm-2 control-label">Talla camisa:</label>
                <div class="col-sm-10">
                        {{ form_widget(form.camisa, { 'attr': {'style':'text-transform:uppercase;','onblur':'javascript:this.value=this.value.toUpperCase()','class': 'form-control'} }) }}
                </div>
            </div>
            <div class="form-group form-group-sm">
                <label for="form_jeans" class="col-sm-2 control-label">Talla jeans:</label>
                <div class="col-sm-10">
                        {{ form_widget(form.jeans, { 'attr': {'style':'text-transform:uppercase;','onblur':'javascript:this.value=this.value.toUpperCase()','class': 'form-control'} }) }}
                </div>
            </div>
            <div class="form-group form-group-sm">
                <label for="form_calzado" class="col-sm-2 control-label">Talla calzado:</label>
                <div class="col-sm-10">
                        {{ form_widget(form.calzado, { 'attr': {'class': 'form-control'} }) }}
                </div>
            </div>    
            <div class="form-group">
                <label for="form_comentarios" class="col-sm-2 control-label">Comentarios:</label>
                <div class="col-sm-10">
                            {{ form_widget(form.comentarios, { 'attr': {'style':'text-transform:uppercase;','onblur':'javascript:this.value=this.value.toUpperCase()','class': 'form-control'} }) }}
                </div>
            </div>                
        </div>
    </div>
    <div class="panel panel-default">
        <div class="panel-heading">Información empresa</div>
        <div class="panel-body">                              
            <div class="form-group form-group-sm">
                <label for="form_cuenta" class="col-sm-2 control-label">Horario:</label>
                <div class="col-sm-10">
                        {{ form_widget(form.horarioRel, { 'attr': {'class': 'form-control'} }) }}
                </div>
            </div>                    
            <div class="form-group form-group-sm">
                <label for="form_departamentoEmpresaRel" class="col-sm-2 control-label">Departamento:</label>
                <div class="col-sm-10">
                        {{ form_widget(form.departamentoEmpresaRel, { 'attr': {'class': 'form-control'} }) }}
                </div>
            </div>                  
        </div>
    </div>            
    <div class="panel panel-default">
        <div class="panel-heading">Información bancaria</div>
        <div class="panel-body">                              
            <div class="form-group form-group-sm">
                <label for="form_cuenta" class="col-sm-2 control-label">Cuenta:</label>
                <div class="col-sm-10">
                        {{ form_widget(form.cuenta, { 'attr': {'class': 'form-control'} }) }}
                </div>
            </div>                
            <div class="form-group form-group-sm">
                <label for="form_tipoCuenta" class="col-sm-2 control-label">Tipo cuenta:</label>
                <div class="col-sm-10">
                        {{ form_widget(form.tipoCuenta, { 'attr': {'class': 'form-control'} }) }}
                </div>
            </div>    
            <div class="form-group form-group-sm">
                <label for="form_bancoRel" class="col-sm-2 control-label">Banco:</label>
                <div class="col-sm-10">
                        {{ form_widget(form.bancoRel, { 'attr': {'class': 'form-control'} }) }}
                </div>
            </div> 
            <div class="form-group form-group-sm">
                <label for="form_auxilioTransporte" class="col-sm-2 control-label">Auxilio transporte:</label>
                <div class="col-sm-10">
                        {{ form_widget(form.auxilioTransporte, { 'attr': {'class': 'form-control'} }) }}
                </div>
            </div>    
            <div class="form-group form-group-sm">
                <label for="form_pagadoEntidadSalud" class="col-sm-2 control-label">Pagado por entidad pensión:</label>
                <div class="col-sm-10">
                        {{ form_widget(form.pagadoEntidadSalud, { 'attr': {'class': 'form-control'} }) }}
                </div>
            </div>                 
        </div>
    </div>
    <div class="panel panel-default">
        <div class="panel-heading">Información clasificacion</div>
        <div class="panel-body">               
            <div class="form-group form-group-sm">
                <label class="col-sm-2 control-label">Tipo:</label>
                <div class="col-sm-10">
                        {{ form_widget(form.empleadoTipoRel, { 'attr': {'class': 'form-control'} }) }}
                </div>
            </div>            
            <div class="form-group form-group-sm">
                <label class="col-sm-2 control-label">Zona:</label>
                <div class="col-sm-10">
                        {{ form_widget(form.zonaRel, { 'attr': {'class': 'form-control'} }) }}
                </div>
            </div> 
            <div class="form-group form-group-sm">
                <label class="col-sm-2 control-label">Sub zona:</label>
                <div class="col-sm-10">
                        {{ form_widget(form.subzonaRel, { 'attr': {'class': 'form-control'} }) }}
                </div>
            </div>                
        </div>
    </div>  
    <div class="panel panel-default">
        <div class="panel-heading">Información contabilidad</div>
        <div class="panel-body">               
            <div class="form-group form-group-sm">
                <label class="col-sm-2 control-label">Grupo pago:</label>
                <div class="col-sm-10">
                        {{ form_widget(form.centroCostoContabilidadRel, { 'attr': {'class': 'form-control'} }) }}
                </div>
            </div>                           
        </div>
    </div>                
    <div class="panel panel-default">
        <div class="panel-heading">Información turnos</div>
        <div class="panel-body">               
            <div class="form-group form-group-sm">
                <label class="col-sm-2 control-label">Puesto:</label>
                <div class="col-sm-10">
                        {{ form_widget(form.puestoRel, { 'attr': {'class': 'form-control'} }) }}
                </div>
            </div>                           
        </div>
    </div>                
    <div class="modal-footer">
        <a class="btn btn-default btn-sm" href="{{ path('brs_rhu_base_empleados_lista') }}">Cancelar</a>
        <a class="btn btn-default btn-sm" href="{{ path('brs_rhu_base_empleados_nuevo_enlazar') }}">Enlazar de registro de seleccion</a>            
        {{ form_widget(form.guardar, { 'attr': {'class': 'btn btn-primary btn-sm'} }) }}
        {{ form_widget(form.guardarnuevo, { 'attr': {'class': 'btn btn-primary btn-sm'} }) }}
    </div>
    {{ form_end(form) }}
</div>
{% endblock %}